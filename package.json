{
  "name": "dev-pack-salesforce",
  "displayName": "Dev Pack for Salesforce",
  "description": "A collection of useful VS Code extensions for Salesforce development",
  "version": "1.0.8",
  "publisher": "avidev9",
  "scripts": {
    "package": "npx vsce package"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "salesforce",
    "vscode",
    "extension",
    "pack"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "extensionPack": [
    "salesforce.salesforcedx-vscode-apex",
    "salesforce.salesforcedx-vscode-lwc",
    "salesforce.salesforcedx-vscode-soql",
    "salesforce.salesforcedx-vscode",
    "davidanson.vscode-markdownlint",
    "esbenp.prettier-vscode",
    "dbaeumer.vscode-eslint",
    "vscode-icons-team.vscode-icons",
    "salesforce.salesforcedx-vscode-core",
    "zhuangtongfa.material-theme",
    "aaron-bond.better-comments",
    "oderwat.indent-rainbow",
    "chuckjonas.apex-pmd",
    "financialforce.lana",
    "wayou.vscode-todo-highlight",
    "streetsidesoftware.code-spell-checker",
    "ForceConfigControl.lightningflowscanner",
    "salesforce.sfdx-code-analyzer-vscode",
    "emilast.LogFileHighlighter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Avinava/vscode-dev-pack-salesforce.git"
  },
  "icon": "assets/icon.jpeg",
  "devDependencies": {
    "@vscode/vsce": "^2.24.0",
    "eslint": "^6.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.8.3",
    "vscode": "^1.1.37"
  },
  "contributes": {
    "commands": [
      {
        "command": "dev-pack-salesforce.forceCheckPackages",
        "title": "Dev Pack for Salesforce: Install required node modules"
      }
    ]
  }
}
